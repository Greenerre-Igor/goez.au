<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title ? title : 'Contact Customer' %>
    </title>
    <link href="https://code.jquery.com/ui/1.10.3/themes/redmond/jquery-ui.css" rel="stylesheet" media="screen">
    <%- include('../../partials/head'); %>
</head>

<body>
    <%- include('../../partials/header'); %>

        <div class="page-content">
            <div class="row">
                <%- include('../../partials/sidebar'); %>
                <div class="col-md-10">
                    <div class="content-box-large">
                        <div class="panel-heading">
                            <div class="panel-title"><%= title ? title : 'Contact Customer' %></div>
                        </div>
                        <div class="panel-body">
                            <div class="col-md-12">
                                <div class="content-box-large">
                                    <div class="panel-heading">
                                    
                                      <!-- <div class="panel-options">
                                        <a href="#" data-rel="collapse"><i class="glyphicon glyphicon-refresh"></i></a>
                                        <a href="#" data-rel="reload"><i class="glyphicon glyphicon-cog"></i></a>
                                      </div> -->
                                  </div>
                                    <div class="panel-body">
                                        <div class="">
                                            <div class="panel-body">
                                                <form class="form-horizontal" role="form">
                                                <div class="form-group">
                                                  <label for="inputEmail3" class="col-sm-2 control-label">Name</label>
                                                  <div class="col-sm-10">
                                                    <input type="text" class="form-control" id="name" value="<%= inquiry.name %>" readonly>
                                                  </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail3" class="col-sm-2 control-label">Email</label>
                                                    <div class="col-sm-10">
                                                      <input type="email" class="form-control" id="email" value="<%= inquiry.email %>" readonly>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail3" class="col-sm-2 control-label">Contact Number</label>
                                                    <div class="col-sm-10">
                                                      <input type="text" class="form-control" id="contact_number" value="<%= inquiry.contact_number %>" readonly>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail3" class="col-sm-2 control-label">Service Type</label>
                                                    <div class="col-sm-10">
                                                      <input type="text" class="form-control" id="contact_number" value="<%= inquiry.service %>" readonly>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputEmail3" class="col-sm-2 control-label">Date Time</label>
                                                    <div class="col-sm-10">
                                                      <input type="text" class="form-control" id="contact_number" value="<%= inquiry.date + ' ' + inquiry.time %>" readonly>
                                                    </div>
                                                </div>
                                                
                                                <div class="form-group">
                                                  <label class="col-sm-2 control-label">Textarea</label>
                                                  <div class="col-sm-10">
                                                    <textarea id="ckeditor_standard"></textarea>

                                                  </div>
                                                </div>
                                              
                                                <div class="form-group">
                                                  <div class="col-sm-offset-2 col-sm-10">
                                                    <button type="submit" class="btn btn-primary">Send</button>
                                                  </div>
                                                </div>
                                              </form>
                                            </div>
                                               
                                          </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <%- include('../../partials/footer'); %>
</body>


<%- include('../../partials/foot'); %>

<script src="/vendors/bootstrap-wysihtml5/lib/js/wysihtml5-0.3.0.js"></script>
<script src="/vendors/bootstrap-wysihtml5/src/bootstrap-wysihtml5.js"></script>

<script src="/vendors/ckeditor/ckeditor.js"></script>
<script src="/vendors/ckeditor/adapters/jquery.js"></script>

<script type="text/javascript" src="/vendors/tinymce/js/tinymce/tinymce.min.js"></script>

<script src="/js/editors.js"></script>

</html>